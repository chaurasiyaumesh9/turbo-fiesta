{
  "name": "turbo-fiesta",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.0"
  },
  "scripts": {
    "client": "cd client && npm run start",
    "build-client": "cd client && npm run build",
    "server": "cd server && npm run start",
    "client-dep": "cd client && npm install && npm run start",
    "server-dep": "cd server && npm install && npm run start",
    "start:dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "start": "cd server && npm install && node ./build/server.js",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
